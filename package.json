{
  "name": "nopodofo",
  "version": "0.6.3",
  "description": "Node.js native module bindings for PoDoFo",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "gypfile": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corymickelson/NoPoDoFo.git"
  },
  "scripts": {
    "docs": "typedoc --options ./tsconfig.docs.json --out ./docs",
    "coverage": "nyc tape spec/unit/*.js && nyc report --reporter=text-lcov | coveralls",
    "test": "mkdir -p spec/tmp && tsc -p tsconfig.json && tsc -p tsconfig.spec.json && node --expose-gc ./node_modules/.bin/tape spec/unit/*.js",
    "install": "node-pre-gyp install --fallback-to-build",
    "qt:debug": "cmake-js build -D -j 4 && cp build/Debug/nopodofo.node lib/binding/1/",
    "prepublish": "tsc -p tsconfig.json"
  },
  "nyc": {
    "exclude": [
      "spec/**"
    ]
  },
  "keywords": [
    "pdf",
    "node"
  ],
  "author": "Cory Mickelson <mickelsoncory@gmail.com> (http://corymickelson.github.io/)",
  "homepage": "https://github.com/corymickelson/NoPoDoFo#readme",
  "bugs": {
    "url": "https://github.com/corymickelson/NoPoDoFo/issues"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "aws-sdk": "^2.252.1",
    "benchmark": "^2.1.4",
    "cmake-js": "^3.7.3",
    "coveralls": "^3.0.1",
    "husky": "^0.14.3",
    "nyc": "^11.9.0",
    "tap-summary": "^4.0.0",
    "tape": "^4.9.0",
    "uuid": "^3.2.1",
    "@types/node": "^8.10.18",
    "@types/tape": "^4.2.32",
    "@types/uuid": "^3.4.3",
    "typescript": "~2.5.3"
  },
  "dependencies": {
    "node-addon-api": "1.3.0",
    "node-pre-gyp": "^0.10.0"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "binary": {
    "module_name": "nopodofo",
    "module_path": "./lib/binding/{napi_build_version}/",
    "remote_path": "napi{napi_build_version}/v{version}",
    "host": "https://nopodofo.s3-us-west-2.amazonaws.com",
    "napi_versions": [
      1
    ]
  },
  "files": [
    "binding.gyp",
    "src",
    "dist",
    "deps",
    "tsconfig.json"
  ]
}
